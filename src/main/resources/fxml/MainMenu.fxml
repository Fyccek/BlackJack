<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="MainPane" prefHeight="562.0" prefWidth="987.0" stylesheets="@../stylesheet/login.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
    <children>
        <Button fx:id="PlayButton" layoutX="183.0" layoutY="337.0" mnemonicParsing="false" onAction="#letsStart" prefHeight="37.0" prefWidth="90.0" text="Play" />
      <Button fx:id="ExitButton" layoutX="14.0" layoutY="511.0" mnemonicParsing="false" onAction="#getOff" prefHeight="37.0" prefWidth="90.0" text="Exit" />
      <Label fx:id="highScore" layoutX="762.0" layoutY="43.0" prefHeight="32.0" prefWidth="100.0" textFill="WHITE" />
      <TextField fx:id="myName" layoutX="134.0" layoutY="231.0" />
      <Label fx:id="nameLabel" layoutX="134.0" layoutY="194.0" prefHeight="30.0" prefWidth="187.0" text="Name:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="submitButton" layoutX="194.0" layoutY="281.0" mnemonicParsing="false" onAction="#submitButtonAction" text="Submit" />
    </children>
</AnchorPane>
